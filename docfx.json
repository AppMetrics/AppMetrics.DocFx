{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/App.Metrics/project.json",
            "**/App.Metrics.Extensions.Middleware/project.json",
            "**/App.Metrics.Extensions.Reporting.Console/project.json",
            "**/App.Metrics.Extensions.Reporting.TextFile/project.json",
            "**/App.Metrics.Formatters.Json/project.json"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "cwd": "../src"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "getting-started/**.md",
          "getting-started/**/toc.yml",          
          "samples/**.md",
          "samples/**/toc.yml",          
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "_exported_templates/default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}