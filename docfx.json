{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/App.Metrics/App.Metrics.csproj",
            "**/App.Metrics.Extensions.Middleware/App.Metrics.Extensions.Middleware.csproj",            
            "**/App.Metrics.Formatters.Json/App.Metrics.Formatters.Json.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "cwd": "../AppMetrics/src"
        }
      ],
      "dest": "api"
    }    
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "getting-started/**.md",
          "getting-started/**/toc.yml",          
          "samples/**.md",
          "samples/**/toc.yml",          
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "dashboards/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "_exported_templates/default"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}